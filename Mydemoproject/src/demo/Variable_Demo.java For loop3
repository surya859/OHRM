package demo;

import java.util.ArrayList;

public class Variable_Demo {

	public static void main(String[] args) 
	{
	  
	 	ArrayList<String>citylist = new ArrayList<>();
	 	citylist.add("Delhi");
	 	citylist.add("Mumbai");
	 	citylist.add("Hyderabad");
	 	citylist.add("Chennai");
	 	citylist.add("Bangalore");
	 	
	 	String citytobeverified = "Hyderabad";
	 	boolean cityexist = false;
	 	
	 	for (int i=0; i<citylist.size(); i++)
	 	{
	 		if (citylist.get(i).equalsIgnoreCase(citytobeverified))
	 		{
	 			cityexist = true;
	 			break;
	 		}	
	 	}	
		if (cityexist)
		{
			System.out.println(citytobeverified + "iscitypresentinthelist,Testpass");
		}else
		{
			System.out.println(citytobeverified + "iscitynotpresentinthelist,Testfail");
		}	
		
		
		
		
		
		
		
		
		
		
		
		
	 	
		
	}

}