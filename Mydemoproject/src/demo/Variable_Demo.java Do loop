package demo;

public class Variable_Demo {

	public static void main(String[] args) 
	{
		
	  int x=101;
	  do
	  {
		  System.out.println(x);
		  x=x+1;
	  }while (x<=100);
	  System.out.println("End of the Do Loop");
	  
	  
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
